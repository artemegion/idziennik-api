{
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        
    ],

    "compilerOptions": {
        "allowUnreachableCode": true,
        "allowUnusedLabels": true,
        "allowJs": false,

        "noImplicitAny": false,
        "noImplicitThis": true,
        "noEmitOnError": true,

        "strict": true,
        
        "sourceMap": false,
        "removeComments": false,
        "experimentalDecorators": false,
        
        "target": "es6",
        "module": "commonjs",
        
        "declaration": true
    }
}